{
    "name": "my-parcel-project",
    "version": "1.0.0",
    "scripts": {
        "watch": "cross-env NODE_ENV=development parcel --out-dir=../assets/static/gen --out-file=main.js --public-url=./assets/ main.js",
        "build": "cross-env NODE_ENV=production parcel build --out-dir=../assets/static/gen --out-file=main.js --public-url=./assets/ main.js"
    },
    "private": true,
    "dependencies": {
        "babel-preset-env": "^1.6.1",
        "cross-env": "^5.1.3",
        "node-sass": "^4.7.2",
        "parcel-bundler": "^1.6.2"
    }
}
